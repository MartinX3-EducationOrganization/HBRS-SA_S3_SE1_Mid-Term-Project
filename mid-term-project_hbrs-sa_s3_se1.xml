  <target name="compile.module.mid-term-project_hbrs-sa_s3_se1" depends="compile.module.mid-term-project_hbrs-sa_s3_se1.production,compile.module.mid-term-project_hbrs-sa_s3_se1.tests" description="Compile module Mid-Term-Project_HBRS-SA_S3_SE1"/>
  
