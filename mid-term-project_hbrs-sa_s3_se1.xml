  <property name="mid-term-project_hbrs-sa_s3_se1.output.dir" value="${module.mid-term-project_hbrs-sa_s3_se1.basedir}/out/production/Mid-Term-Project_HBRS-SA_S3_SE1"/>
  <property name="mid-term-project_hbrs-sa_s3_se1.testoutput.dir" value="${module.mid-term-project_hbrs-sa_s3_se1.basedir}/out/test/Mid-Term-Project_HBRS-SA_S3_SE1"/>
  
  <path id="mid-term-project_hbrs-sa_s3_se1.module.bootclasspath">
