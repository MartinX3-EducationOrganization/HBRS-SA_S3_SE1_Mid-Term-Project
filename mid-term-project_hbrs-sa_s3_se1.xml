      <patternset refid="excluded.from.compilation.mid-term-project_hbrs-sa_s3_se1"/>
    </javac2>
    
    <copy todir="${mid-term-project_hbrs-sa_s3_se1.output.dir}">
      <fileset dir="${module.mid-term-project_hbrs-sa_s3_se1.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.mid-term-project_hbrs-sa_s3_se1.tests" depends="register.custom.compilers,compile.module.mid-term-project_hbrs-sa_s3_se1.production" description="compile module Mid-Term-Project_HBRS-SA_S3_SE1; test classes" unless="skip.tests"/>
  
