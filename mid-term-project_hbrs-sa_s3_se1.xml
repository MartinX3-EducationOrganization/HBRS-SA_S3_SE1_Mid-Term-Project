<?xml version="1.0" encoding="UTF-8"?>
<project name="mid-term-project_hbrs-sa_s3_se1" default="all">
  
  
  <property file="mid-term-project_hbrs-sa_s3_se1.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
