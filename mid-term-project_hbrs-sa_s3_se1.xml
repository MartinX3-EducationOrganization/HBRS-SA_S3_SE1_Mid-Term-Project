  <target name="clean" depends="clean.module.mid-term-project_hbrs-sa_s3_se1" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.mid-term-project_hbrs-sa_s3_se1" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
